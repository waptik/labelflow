{
  "name": "@labelflow/db",
  "version": "0.0.1",
  "main": "src/index.ts",
  "scripts": {
    "dev:server": "nodemon src/dev/apollo-server.ts",
    "dev": "dotenv -e .env.development -e .env.local prisma migrate dev",
    "build": "dotenv -e .env.development -e .env.local prisma generate",
    "start": "dotenv -e .env.development -e .env.local -- docker compose up --detach && sleep 10 && yarn deploy:all",
    "stop": "dotenv -e .env.development -e .env.local -- docker compose down --volumes --remove-orphans",
    "deploy": "dotenv -e .env.development -e .env.local prisma migrate deploy",
    "prisma:production": "dotenv -e .env.production prisma",
    "deploy:test": "dotenv -e .env.test prisma migrate deploy",
    "deploy:all": "yarn deploy && yarn deploy:test",
    "dev:push": "dotenv -e .env.development -e .env.local prisma db push",
    "generate": "dotenv -e .env.development -e .env.local prisma generate"
  },
  "devDependencies": {
    "@apollo/client": "3.4.16",
    "@types/node": "16.4.1",
    "dotenv-cli": "4.0.0",
    "nodemon": "2.0.15",
    "prisma": "2.30.3",
    "ts-node": "10.1.0",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.24.0",
    "@aws-sdk/s3-request-presigner": "3.24.0",
    "@graphql-tools/graphql-file-loader": "6.2.7",
    "@graphql-tools/load": "6.2.8",
    "@graphql-tools/merge": "6.2.14",
    "@graphql-tools/schema": "7.1.5",
    "@labelflow/common-resolvers": "workspace:typescript/common-resolvers",
    "@labelflow/graphql-types": "workspace:typescript/graphql-types",
    "@labelflow/utils": "workspace:typescript/utils",
    "@prisma/client": "2.30.3",
    "@supabase/supabase-js": "1.30.6",
    "apollo-server": "3.5.0",
    "fetch-blob": "3.1.3",
    "graphql": "15.5.1",
    "isomorphic-fetch": "3.0.0",
    "jimp": "0.16.1",
    "lodash": "4.17.21",
    "memoize-one": "5.2.1",
    "slugify": "1.6.0",
    "stripe": "8.184.0",
    "type-fest": "2.11.1",
    "uuid": "8.3.2"
  }
}
